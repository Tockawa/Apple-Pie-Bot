{
    "name": "apple-pie",
    "version": "1.7.0",
    "description": "Just an usual Discord Bot, she might be a pie made of apples, but she's also a bot.",
    "main": "dist/src/bot.js",
    "scripts": {
        "start": "node dist/src/bot.js",
        "postinstall": "npm rebuild ./node_modules",
        "dev": "nodemon",
        "dev-start": "ts-node src/bot.ts",
        "build": "tsc -p tsconfig.json"
    },
    "keywords": [
        "discord",
        "bot",
        "typescript",
        "apple pie",
        "discord.js",
        "discordjs",
        "djs"
    ],
    "author": "тоска",
    "contributors": [
        "Tockawa",
        "Dixon"
    ],
    "license": "MIT",
    "dependencies": {
        "bcrypt": "^5.1.0",
        "cheerio": "^1.0.0-rc.12",
        "cron": "^2.2.0",
        "discord.js": "^14.7.1",
        "dotenv": "^16.0.3",
        "module-alias": "^2.2.2",
        "moment": "^2.29.4",
        "mongoose": "^6.9.0",
        "node-fetch": "^2.6.7",
        "request": "^2.88.2",
        "speakeasy": "^2.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5",
        "undici": "^5.16.0"
    },
    "devDependencies": {
        "@discordjs/rest": "^1.5.0",
        "@napi-rs/canvas": "^0.1.34",
        "@types/bcrypt": "^5.0.0",
        "@types/cron": "^2.0.0",
        "@types/node": "^18.11.18",
        "@types/speakeasy": "^2.0.7"
    },
    "nodemonConfig": {
        "ignore": [
            "node_modules",
            "dist"
        ],
        "watch": [
            "src",
            "types",
            "modules",
            "package.json"
        ],
        "ext": "ts,json",
        "exec": "npm run dev-start",
        "execMap": {
            "ts": "ts-node"
        },
        "verbose": true,
        "execOptions": {
            "env": {
                "NODE_ENV": "development"
            }
        }
    },
    "_moduleAliases": {
        "@wokcommands": "dist/modules/wokcommands/src/index.js",
        "@cliprxyz": "dist/modules/cliprxyz/index.js"
    }
}
