        "@types/bcrypt": "^5.0.0",
        "@types/cron": "^2.0.0",
        "@types/moment": "^2.13.0",
        "@types/node": "^18.11.9",
        "@types/speakeasy": "^2.0.7",
        "bcrypt": "^5.1.0",
        "cheerio": "^1.0.0-rc.12",
        "cliprxyz": "file:modules/dist/cliprxyz",
        "cron": "^2.1.0",
        "discord.js": "^14.6.0",
        "dotenv": "^16.0.3",
